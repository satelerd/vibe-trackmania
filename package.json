{
  "name": "vibe-trackmania",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:report": "node scripts/build-report.cjs",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test -c playwright.config.cjs",
    "test:all": "pnpm test && pnpm test:e2e"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.19.3",
    "lil-gui": "^0.21.0",
    "postprocessing": "^6.38.2",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@types/three": "^0.182.0",
    "typescript": "~5.9.3",
    "vite": "^6.4.0",
    "vitest": "^4.0.18"
  }
}
